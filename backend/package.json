{
  "name": "backend",
  "version": "0.1.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/typeorm.datasource.ts src/migrations/auto",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/typeorm.datasource.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/typeorm.datasource.ts",
    "poststart": "node dist/scripts/runMigrations.js"
  },
  "dependencies": {
    "@nestjs/config": "^3.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "file-type": "^18.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.13.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^14.2.0",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^9.0.7",
    "@types/pdfkit": "^0.13.6",
    "@types/multer": "^1.4.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
